{
  "family": "hospital-etl",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "4096",
  "executionRoleArn": "arn:aws:iam::665277163763:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::665277163763:role/HospitalETLTaskRole", 
  "containerDefinitions": [
    {
      "name": "hospital-etl",
      "image": "665277163763.dkr.ecr.us-east-1.amazonaws.com/hospital-etl:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/hospital-etl",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
